        private Thread MakeUnique_Thread = null;
        private Thread Intersect_Thread = null; 
        private Thread Sorting_Thread = null;
        
        private void UI_Button_Click(object sender, EventArgs e)
        {
            bool success = false;
            
            List<float> temp1 = new List<float>();
            List<float> temp2 = new List<float>();
            List<List<float>> listTemp = new List<List<float>>((int)UI_Generate_List.Value);
            
            success = Error();
            if (success)
            {
                
                Sorting_Thread = new Thread(BubbleSort);
                Sorting_Thread.IsBackground = true;
                Sorting_Thread.Start(listTemp);
                
                MakeUnique_Thread = new Thread(MakeUniqueList);
                MakeUnique_Thread.IsBackground = true;
                MakeUnique_Thread.Start(listTemp);

                Intersect_Thread = new Thread(delegate(){IntersectList(temp1,temp2);});
                Intersect_Thread.IsBackground = true;
                Intersect_Thread.Start(); 
            }
        }
        
