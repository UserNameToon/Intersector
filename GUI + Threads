        private bool Error()       //Error message for the programs 
        {
            bool success = false;
            if (UI_Generate_Length.Value <= 0)
            {
                MessageBox.Show("Please enter the value you wish to generate");
            }
            else if (UI_Generate_List.Value <= 0)
            {
                MessageBox.Show("Please enter the amount of list you wish to creates");
            }
            else
            {
                success = true;
            }
            return success;
        }
